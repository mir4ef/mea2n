{
  "name": "mea2n",
  "description": "(M)EAN Stack with Angular 2+ Starter",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "author": "My Name <my@email.com>",
  "homepage": "https://example.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mir4ef/mea2n.git"
  },
  "bugs": {
    "url": "https://github.com/mir4ef/mea2n/issues"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "main": "server.js",
  "directories": {
    "app": "src",
    "node": "server"
  },
  "scripts": {
    "postinstall": "npm run copy:swagger",
    "start": "npm run docs:api && npm run docs:app && ng serve --ssl",
    "build": "npm run docs:api && npm run docs:app && ng build",
    "build:prod": "npm run security:scan && npm run docs:api && npm run docs:app && npm run lint && npm test && ng build --prod --aot",
    "build:watch": "ng build -w",
    "test": "npm run test:node:coverage && npm run test:ng -- --single-run --code-coverage",
    "test:ng": "ng test",
    "test:node": "node jasmine/helpers/reporter.js",
    "test:node:coverage": "node ./node_modules/nyc/bin/nyc.js npm run test:node",
    "e2e": "ng e2e",
    "lint": "npm run lint:css && npm run lint:node && npm run lint:ts",
    "lint:css": "node ./node_modules/lesshint/bin/lesshint ./src/",
    "lint:node": "node ./node_modules/eslint/bin/eslint.js ./server cluster.js server.js",
    "lint:ts": "ng lint --type-check",
    "docs:app": "node ./node_modules/typedoc/bin/typedoc --out ./src/documentation/app/ --mode file --module \"node\" --target \"ES5\" ./src/app/",
    "docs:api": "node ./node_modules/swagger-jsdoc/bin/swagger-jsdoc.js -d ./server/swagger.def.js -o ./src/documentation/api/swagger.json ./server/routes/v1/*.js ./server/routes/v1/**/*.js",
    "docs:api:watch": "npm run docs:api -- -w",
    "copy:swagger": "node ./node_modules/copyfiles/copyfiles -f \"./node_modules/swagger-ui-dist/sw*.js*\" \"./node_modules/swagger-ui-dist/sw*.css*\" ./src/documentation/api",
    "security:scan": "node ./node_modules/nsp/bin/nsp check",
    "pre-git": "npm run e2e && npm run build:prod"
  },
  "pre-commit": [
    "pre-git"
  ],
  "pre-push": [
    "pre-git"
  ],
  "dependencies": {
    "@angular/animations": "^4.2.5",
    "@angular/common": "^4.2.5",
    "@angular/compiler": "^4.2.5",
    "@angular/core": "^4.2.5",
    "@angular/forms": "^4.2.5",
    "@angular/http": "^4.2.5",
    "@angular/platform-browser": "^4.2.5",
    "@angular/platform-browser-dynamic": "^4.2.5",
    "@angular/router": "^4.2.5",
    "body-parser": "^1.17.0",
    "chalk": "^2.0.1",
    "compression": "^1.7.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "express": "^4.15.0",
    "express-rate-limit": "^2.8.0",
    "helmet": "^3.6.0",
    "intl": "^1.2.5",
    "jsonwebtoken": "^7.4.0",
    "morgan": "^1.8.0",
    "rxjs": "^5.4.2",
    "spdy": "^3.4.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.0",
    "@angular/compiler-cli": "^4.2.5",
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.7",
    "codelyzer": "^3.1.2",
    "copyfiles": "^1.2.0",
    "eslint": "~4.2.0",
    "jasmine": "~2.6.0",
    "jasmine-core": "~2.6.1",
    "jasmine-node": "~1.14.5",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "lesshint": "^4.0.0",
    "nsp": "^2.6.3",
    "nyc": "^11.0.3",
    "phantomjs-prebuilt": "^2.1.14",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "protractor": "~5.1.0",
    "swagger-jsdoc": "^1.9.5",
    "swagger-ui-dist": "^3.0.17",
    "ts-node": "~3.2.0",
    "tslint": "~5.5.0",
    "typedoc": "~0.7.0",
    "typescript": "~2.3.0"
  }
}
